ENVIRONMENT=test


#====================================================
# Credentials from your Hipay's Security Settings
# In your Hipay's account, "Integration > Security Settings > Api Credential"
#=====================================================
HIPAY_API_USER_TEST={HIPAY_API_USER_TEST}
HIPAY_API_PASSWORD_TEST={HIPAY_API_PASSWORD_TEST}
HIPAY_TOKENJS_PUBLICKEY_TEST={HIPAY_TOKENJS_PUBLICKEY_TEST}
HIPAY_TOKENJS_USERNAME_TEST={HIPAY_TOKENJS_USERNAME_TEST}
HIPAY_SECRET_PASSPHRASE_TEST={HIPAY_SECRET_PASSPHRASE_TEST}

HIPAY_API_USER_TEST_MOTO={HIPAY_API_USER_TEST_MOTO}
HIPAY_API_PASSWORD_TEST_MOTO={HIPAY_API_PASSWORD_TEST_MOTO}
HIPAY_SECRET_PASSPHRASE_TEST_MOTO={HIPAY_SECRET_PASSPHRASE_TEST_MOTO}

#====================================================
# Default payments methods activated at startup
#=====================================================
ACTIVE_METHODS=hipay_cc,hipay_hosted,hipay_hosted_fields

#====================================================
# Conf for debugging
#=====================================================
XDEBUG_CONFIG=

#====================================================
# Configuration PI DATA URL
#=====================================================
PI_DATA_URL={PI_CI_DATA_URL}
